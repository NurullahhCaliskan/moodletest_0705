<footer id="dashboard_footer03" class="footer bg-thm-color-one style_2 style_3 z-1" 
    style="background-image:url({{frontpageimglink}}elements/element_18.png);">
    <div class="container relative z-1">
        <div class="footer_top section-padding">
            <div class="row">
                <div class="col-lg-3 col-md-6">
                    <div class="ft_widget ft_about">
                        <a href="#" class="logo">
                            {{#footer_logo_sm}}
                            {{#output.get_compact_logo_url}}
                                <img src="{{frontpageimglink}}logo-white.png" alt="{{sitename}}" class="image-fit">
                            {{/output.get_compact_logo_url}} 
                            {{^output.get_compact_logo_url}}
                                <div class="footer-site-name">
                                    <div class="h1">{{{footer_col1_header}}}</div>
                                </div>
                            {{/output.get_compact_logo_url}}
                            {{/footer_logo_sm}}

                            {{#footer_logo}}
                            {{#output.get_logo_url}}     
                                <img src="{{frontpageimglink}}logo-white.png" alt="{{sitename}}">
                            {{/output.get_logo_url}} 
                            {{^output.get_logo_url}}
                                <div class="footer-site-name">
                                    <div class="h1">{{{footer_col1_header}}}</div>
                                </div> 
                            {{/output.get_logo_url}} 
                            {{/footer_logo}}

                            {{^footer_logo_sm}}
                            {{^footer_logo}}
                            <div class="footer-site-name">
                                <div class="h1">{{{footer_col1_header}}}</div>
                            </div> 
                            {{/footer_logo}} 
                            {{/footer_logo_sm}}  
                        </a>
                        <ul class="info">
                            {{{footer_col1_caption}}}
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="ft_widget ft_menu">
                        <h6 class="ft-title">{{footer_col2_header}}</h6>
                        <ul>
                            {{{footer_col2_link}}}
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="ft_widget ft_menu ">
                        <h6 class="ft-title">{{footer_col3_header}}</h6>
                        <ul>
                            {{{footer_col3_link}}}
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="ft_widget ft_newsletter" >
                        <h6 class="ft-title">{{footer_col4_header}}</h6>
                        <p class="mb-xl-20">{{{footer_col4_caption}}}</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer_bottom d-flex justify-content-center">
            {{> theme_wellko/frontpage/setup-button }}  
            <p class="mb-0">
                {{{footer_copyright}}}
            </p>
        </div>
        <div class="arrows to_up slideTop">
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
        </div>
    </div>
</footer>