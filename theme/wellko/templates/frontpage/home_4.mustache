{{!
    @template theme_wellko/frontpage/home4

    Theme front page builder.
}}

{{> theme_wellko/frontpage/head }}

<body {{{ bodyattributes }}}>
    <!-- Start preloader -->
    {{> theme_wellko/frontpage/preloader }}
    <!-- End preloader -->

    {{> core/local/toast/wrapper}}

    <div class="frontpage_overflow">
        <!-- Start Header Area -->
            {{> theme_wellko/frontpage/navbar/header2}}
        <!-- End Header Area -->
        <!-- HOME ONE BLOCKS -->
        {{#h4_01_block_enabled}}
            {{> theme_wellko/frontpage/home_4/block_01 }}
        {{/h4_01_block_enabled}}
        {{#h4_02_block_enabled}}
            {{> theme_wellko/frontpage/home_4/block_02 }}
        {{/h4_02_block_enabled}}
  
        {{#footer_enabled}}
            {{> theme_wellko/frontpage/other/footer02 }}
        {{/footer_enabled}}
        {{#aside_enabled}}
            {{> theme_wellko/frontpage/other/aside }}
        {{/aside_enabled}}

        {{> theme_wellko/frontpage/other/aside_m }}
        <!-- Start dark-light -->
        {{> theme_wellko/frontpage/darklight }}
        <!-- End dark-light -->
        <!-- Start go to top -->
            {{> theme_wellko/frontpage/gototop }}
        <!-- End go to top -->
        {{> theme_wellko/frontpage/setup}}
    </div>
</body>   
</html>
{{#js}}
    M.util.js_pending('theme_boost/loader');
    require(['theme_boost/loader'], function() {
        M.util.js_complete('theme_boost/loader');
    });
{{/js}}
