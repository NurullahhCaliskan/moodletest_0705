                            {{/imgurl}}
                        </div>
                        <div class="post_caption">
                            <ul class="post_meta">
                                <li>
                                    <i class="fal fa-calendar-alt"></i>
                                    {{lastmodified}}
                                </li>
                                <li>
                                    <i class="fal fa-comment-lines"></i>
                                    <span lang="en" class="multilang" style="display:none;">Comments</span>
                            <span lang="tr" class="multilang" style="display:none;">Yorum</span> ({{commentscount}}) 
                                </li>
                            </ul>
                            <h4 class="post_title">
                                <a href="{{post_detail_lnk}}">{{subject}}</a>
                            </h4>
                            <p class="post_text">
                               {{#shortentext}} 125, {{{summary}}} {{/shortentext}}
                            </p>
                            <a href="{{post_detail_lnk}}" class="thm-btn bg-thm-color-white thm-color-one btn-rectangle">
                            <span lang="en" class="multilang" style="display:none;">Read More</span>
                            <span lang="tr" class="multilang" style="display:none;">Daha Fazla</span>
                            <i class="fal fa-chevron-right ml-2"></i></a>
                        </div>
                    </article>
                </div>
                {{/h3_11_block}}
                <!-- post -->
            </div>
        </div>
    </section>
