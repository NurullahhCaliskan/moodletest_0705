{{!
    @template theme_wellko/frontpage/home2

    Theme front page builder.
}}

{{> theme_wellko/frontpage/head }}

<body {{{ bodyattributes }}}>
    <!-- Start preloader -->
    {{> theme_wellko/frontpage/preloader }}
    <!-- End preloader -->

    {{> core/local/toast/wrapper}}

    <div class="frontpage_overflow">
        <!-- Start Header Area -->
            {{> theme_wellko/frontpage/navbar/header3}}
        <!-- End Header Area -->
        <!-- HOME ONE BLOCKS -->
        {{#h3_02_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_02 }}
        {{/h3_02_block_enabled}}
        {{#h3_03_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_03 }}
        {{/h3_03_block_enabled}}
        {{#h3_04_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_04 }}
        {{/h3_04_block_enabled}}
        {{#h3_05_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_05 }}
        {{/h3_05_block_enabled}}
        {{#h3_06_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_06 }}
        {{/h3_06_block_enabled}}
        {{#h3_07_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_07 }}
        {{/h3_07_block_enabled}}
        {{#h3_08_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_08 }}
        {{/h3_08_block_enabled}}
        {{#h3_09_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_09 }}
        {{/h3_09_block_enabled}}
        {{#h3_10_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_10 }}
        {{/h3_10_block_enabled}}
        {{#h3_11_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_11 }}
        {{/h3_11_block_enabled}}
        {{#h3_12_block_enabled}}
            {{> theme_wellko/frontpage/home_3/block_12 }}
        {{/h3_12_block_enabled}}
        {{#footer_enabled}}
            {{> theme_wellko/frontpage/other/footer03 }}
        {{/footer_enabled}}

        {{> theme_wellko/frontpage/other/aside_m }}
        <!-- Start dark-light -->
        {{> theme_wellko/frontpage/darklight }}
        <!-- End dark-light -->
        <!-- Start go to top -->
            {{> theme_wellko/frontpage/gototop }}
        <!-- End go to top -->
        {{> theme_wellko/frontpage/setup}}
    </div>
</body>   
</html>
{{#js}}
    M.util.js_pending('theme_boost/loader');
    require(['theme_boost/loader'], function() {
        M.util.js_complete('theme_boost/loader');
    });
{{/js}}
