    <section id="h1_block07" class="section-padding pt-0 relative z-1">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title left-align wow fadeInLeft">
                        <p class="subtitle mb-4">
                            <i class="fal fa-book"></i>
                            {{h1_07_block_headline}}
                        </p>
                        <h3 class="title mb-0">{{h1_07_block_header}}<br> {{h1_07_block_maintitle}}</h3>
                    </div>
                </div>
            </div>
            <div class="testimonial_slider row arrows_none">
                <!-- Testimonial Item -->
                {{#h1_07_block}}
                <div class="slide-item col-12">
                    <div class="testimonial_item wow fadeInUp" data-wow-delay=".{{counter}}s">
                        <div class="author">
                            <div class="image bg-thm-color-two">
                                <img src="{{frontpageimglink}}author/{{counter}}.jpg" alt="img" class="image-fit">
                            </div>
                            <div class="text">
                                <h6 class="mb-0">{{name}}</h6>
                                <p class="mb-0 font-weight-bold thm-color-two">{{job}}</p>
                            </div>
                        </div>
                        <p class="comment">
                            {{comment}}
                        </p>
                        <div class="ratings">
                            <i class="fal fa-star {{rating1}}"></i>
                            <i class="fal fa-star {{rating2}}"></i>
                            <i class="fal fa-star {{rating3}}"></i>
                            <i class="fal fa-star {{rating4}}"></i>
                            <i class="fal fa-star {{rating5}}"></i>
                        </div>
                    </div>
                </div>
                {{/h1_07_block}}
                <!-- Testimonial Item -->
            </div>
        </div>
        <div class="circle_wave"></div>
    </section>
