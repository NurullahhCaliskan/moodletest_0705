<!-- Skills -->
{{#page_06_s1_enabled}}
<section class="section-padding side_bg" style="background-image: url({{frontpageimglink}}bg/bg_5.png);">
    <div class="container">
        <div class="section-padding pt-0">
            <div class="row align-items-center justify-content-between">
                <div class="col-xl-6 col-lg-6 mb-md-80">
                    <div class="section-title left-align wow fadeInDown">
                        <p class="subtitle">
                            <i class="fal fa-book"></i>
                            {{h3_05_block_headline}}
                        </p>
                        <h3 class="title wow ">{{h3_05_block_header}}</h3>
                        <p>{{h3_05_block_maintitle}}</p>
                        <a href="?page=2" class="thm-btn bg-thm-color-two thm-color-two-shadow btn-rectangle">Learn More 
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-5">
                    <div class="row feature-masonry">
                        <div class="col-sm-6 grid-item wow fadeInUp">
                            <div class="progress_box grid">
                                <div class="circle_bar" data-percent="{{h3_05_block_circle_num_1}}" data-track-color="#ecf2ff" data-bar-color="#4b83fb" data-size="80">
                                    <div class="counter transform-center text-center">
                                        <b data-from="0" data-to="{{h3_05_block_circle_num_1}}">0</b>
                                    </div>
                                </div>
                                <div class="text">
                                    <h5 class="mb-0">{{h3_05_block_circle_txt1_1}}<br>
                                        {{h3_05_block_circle_txt2_1}}</h5>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 grid-item">
                            <div class="progress_box grid wow fadeInDown">
                                <div class="circle_bar" data-percent="{{h3_05_block_circle_num_2}}" data-track-color="#ecf2ff" data-bar-color="#65cfa9" data-size="80">
                                    <div class="counter transform-center text-center">
                                        <b data-from="0" data-to="{{h3_05_block_circle_num_2}}">0</b>
                                    </div>
                                </div>
                                <div class="text">
                                    <h5 class="mb-0">{{h3_05_block_circle_txt1_2}}<br>
                                        {{h3_05_block_circle_txt2_2}}</h5>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 grid-item">
                            <div class="progress_box grid wow fadeInUp">
                                <div class="circle_bar" data-percent="{{h3_05_block_circle_num_3}}" data-track-color="#ecf2ff" data-bar-color="#ffbd3f" data-size="80">
                                    <div class="counter transform-center text-center">
                                        <b data-from="0" data-to="{{h3_05_block_circle_num_3}}">0</b>
                                    </div>
                                </div>
                                <div class="text">
                                    <h5 class="mb-0">{{h3_05_block_circle_num_3}}<br>
                                        {{h3_05_block_circle_txt2_3}}</h5>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 grid-item text-center">
                            <img src="{{frontpageimglink}}elements/transparent_shape.png" alt="icon">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <hr class="m-0">
    </div>
    <!-- Counter Box Start -->
    <div class="section-padding pb-0">
        <div class="container">
            <div class="row">
                {{#h2_02_block}}
                <!-- Box -->
                <div class="{{col}}">
                    <div class="counter_box wow fadeInUp" data-wow-delay=".{{counter4}}s">
                        <div class="mr-5">
                            <img src="{{frontpageimglink}}icons/icon_{{counter4}}.png" alt="icon" class="image-fit">
                        </div>
                        <div class="text">
                            <div class="counter">
                                <b data-from="0" data-to="{{counting}}">{{counting}}</b>
                            </div>
                            <p class="mb-0">{{{title}}}</p>
                        </div>
                    </div>
                </div>
                <!-- Box -->
                {{/h2_02_block}}
            </div>
        </div>
    </div>
    <!-- Counter Box End -->
</section>
{{/page_06_s1_enabled}}
<!-- Skills End -->
<!-- Team Start -->
{{#page_06_s2_enabled}}
<section class="section-padding bg-thm-color-two-gradient style_2 z-1 team_page_bg" style="background-image: url({{frontpageimglink}}bg/bg_6.png);">
    <img src="{{frontpageimglink}}elements/element_31.png" class="element_1" alt="Element">
    <img src="{{frontpageimglink}}elements/element_32.png" class="element_2" alt="Element">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="section-title wow fadeInDown">
                    <p class="subtitle">
                        <i class="fal fa-book"></i>
                        {{h1_04_block_headline}}
                    </p>
                    <h3 class="title">{{h1_04_block_header}}</h3>
                </div>
            </div>
        </div>
        <div class="row">
            <!-- Block Start -->
            {{#h1_04_block}}
            <div class="col-lg-3 col-md-6">
                <div class="team_block style_2 style_3 wow fadeInUp" data-wow-delay=".{{counter3}}ms">
                    <div class="team_img">
                        {{#userpictureURL}}
                            <img src="{{userpictureURL}}" alt="img" class="image-fit">
                        {{/userpictureURL}}
                        <a href="{{team_detail_lnk}}" class="thm-btn bg-thm-color-two thm-color-two-shadow btn-circle link">
                            <i class="fal fa-plus"></i>
                        </a>
                    </div>
                    <h6 class="mb-1"><a href="{{team_detail_lnk}}">{{teachername}}</a></h6>
                    <p class="thm-color-two mb-0 font-weight-bold">{{job}}</p>
                </div>
            </div>
            {{/h1_04_block}}
            <!-- Block End -->
            <!-- Block Start -->
            <div class="col-lg-3 col-md-6">
                <div class="team_block style_2 style_3 add_team wow fadeInDown" data-wow-delay=".120ms">
                    <a href="?page=10" class="video_btn video_text popup-youtube">
                        <i class="fal fa-plus video_icon bg-thm-color-three thm-btn p-0 thm-color-three-shadow"></i>
                    </a>
                </div>
            </div>
            <!-- Block End -->
        </div>
    </div>
</section>
