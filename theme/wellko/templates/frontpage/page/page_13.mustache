<!-- Blog list Start -->
{{#page_13_s1_enabled}}
<div id="page_13_s1" class="section-padding">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 mb-md-80">
                <!-- post standard -->
                {{#page_13_s1_style_1_enabled}}
                {{#h2_09_block}}
                <article class="post post_list style_2 wow fadeInDown">
                    {{#imgurl}}
                        <img src="{{imgurl}}" alt="img" class="image-fit">
                    {{/imgurl}}
                    <div class="post_caption">
                        <ul class="post_meta">
                            <li>
                                <i class="fal fa-user"></i>
                                {{username}}
                            </li>
                            <li>
                                <i class="fal fa-calendar-alt"></i>
                                {{lastmodified}}
                            </li>
                            <li>
                                <i class="fal fa-comment-lines"></i>
                                Comments ({{commentscount}})
                            </li>
                        </ul>
                        <h4 class="post_title">
                            <a href="{{post_detail_lnk}}">{{subject}}</a>
                        </h4>
                        <p class="post_text">
                           {{#shortentext}} 400, {{{summary}}} {{/shortentext}}
                        </p>
                        <a href="{{post_detail_lnk}}" class="thm-btn bg-thm-color-white thm-color-one btn-rectangle">
                            {{page_13_s1_button}} 
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </article>
                {{/h2_09_block}}
                {{/page_13_s1_style_1_enabled}}
                <!-- post standard -->
                <!-- post video -->
                {{#page_13_s1_style_2_enabled}}
                {{#h2_09_block}}
                <article class="post post_list style_2 post_video wow fadeInUp" data-wow-delay=".20ms">
                    <div class="post_img">
                        <img src="{{imgurl}}" alt="img" class="image-fit">
                        <a href="{{page_13_s2_video_link}}" class="popup-youtube video_btn transform-center justify-content-center d-flex style_2">
                            <i class="fas fa-play video_icon bg-thm-color-three pulse-animated"></i>
                        </a>
                    </div>
                    <div class="post_caption">
                        <ul class="post_meta">
                            <li>
                                <i class="fal fa-user"></i>
                                {{username}}
                            </li>
                            <li>
                                <i class="fal fa-calendar-alt"></i>
                                {{lastmodified}}
                            </li>
                            <li>
                                <i class="fal fa-comment-lines"></i>
                                Comments ({{commentscount}})
                            </li>
                        </ul>
                        <h4 class="post_title">
                            <a href="{{post_detail_lnk}}">{{subject}}</a>
                        </h4>
                        <p class="post_text">
                            {{#shortentext}} 400, {{{summary}}} {{/shortentext}}                        
                        </p>
                        <a href="{{post_detail_lnk}}" class="thm-btn bg-thm-color-white thm-color-one btn-rectangle">
                            {{page_13_s1_button}}
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </article>
                {{/h2_09_block}}
                {{/page_13_s1_style_2_enabled}}
                <!-- post video -->
                <!-- post no thumb -->
                {{#page_13_s1_style_3_enabled}}
                {{#h2_09_block}}
                <article class="post post_list style_2 no-thumb wow fadeInDown" data-wow-delay=".30ms">
                    <div class="post_caption">
                        <ul class="post_meta">
                            <li>
                                <i class="fal fa-user"></i>
                                {{username}}
                            </li>
                            <li>
                                <i class="fal fa-calendar-alt"></i>
                                {{lastmodified}}
                            </li>
                            <li>
                                <i class="fal fa-comment-lines"></i>
                                Comments ({{commentscount}})
                            </li>
                        </ul>
                        <h4 class="post_title">
                            <a href="{{post_detail_lnk}}">{{subject}}</a>
                        </h4>
                        <p class="post_text">
                            {{#shortentext}} 400, {{{summary}}} {{/shortentext}}
                        </p>
                        <a href="{{post_detail_lnk}}" class="thm-btn bg-thm-color-white thm-color-one btn-rectangle">
                            {{page_13_s1_button}}
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </article>
                {{/h2_09_block}}
                {{/page_13_s1_style_3_enabled}}
                <!-- post no thumb -->
                <!-- Post Bg -->
                {{#page_13_s1_style_4_enabled}}
                {{#h2_09_block}}
                <article class="post post_list style_2 with_bg wow fadeInDown"  data-wow-delay=".50ms" 
                    style="background-image: url({{frontpageimglink}}blog/bg.jpg);">
                    <div class="post_caption">
                        <ul class="post_meta">
                            <li>
                                <i class="fal fa-user"></i>
                                {{username}}
                            </li>
                            <li>
                                <i class="fal fa-calendar-alt"></i>
                                {{lastmodified}}
                            </li>
                            <li>
                                <i class="fal fa-comment-lines"></i>
                                Comments ({{commentscount}})
                            </li>
                        </ul>
                        <h4 class="post_title">
                            <a href="{{post_detail_lnk}}">{{subject}}</a>
                        </h4>
                        <a href="{{post_detail_lnk}}" class="thm-btn bg-thm-color-two thm-color-one btn-rectangle">
                            {{page_13_s1_button}}
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </article>
                <!-- Post Bg -->
                {{/h2_09_block}}
                {{/page_13_s1_style_4_enabled}}
            </div>
            <div class="col-lg-4">
                <div class="sidebar style_2">
                    <div class="sidebar_widget wow fadeInUp">
                        <form action="search">
                            <div class="input-group search_widget">
                                <input type="text" name="q" placeholder="Search" class="form-control" autocomplete="off" required="">
                                <div class="input-group-append ml-0">
                                    <button type="submit" class="thm-btn bg-thm-color-two thm-color-white thm-color-two-shadow btn-rectangle btn-small">
                                        <i class="fal fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="sidebar_widget recent_widgets wow fadeInUp">
                        <h5 class="widget_title">{{page_13_page_sidebar_header_1}}</h5>
                        <ul>
                            {{#h2_09_block}}
                            <li>
                                <div class="image">
                                    {{#imgurl}}
                                        <img src="{{imgurl}}" alt="img" class="image-fit">
                                    {{/imgurl}}
                                </div>
                                <div class="text">
                                    <h6 class="mb-0">
                                            <a href="{{post_detail_lnk}}">{{subject}}</a>
                                    </h6>
                                    <p class="mb-0"><strong><i class="fal fa-calendar-alt"></i> </strong>
                                        <a href="{{post_detail_lnk}}">{{created}}</a>
                                    </p>
                                </div>
                            </li>
                            {{/h2_09_block}}
                        </ul>
                    </div>
                    <div class="sidebar_widget wow fadeInUp">
                        <a href="{{page_13_page_sidebar_adimg_lnk}}" class="d-block">
                            <img src="{{frontpageimglink}}blog/adv.jpg" alt="adv" class="image-fit">
                        </a>
                    </div>
                    <div class="sidebar_widget wow fadeInUp">
                        <h5 class="widget_title">{{page_13_page_sidebar_header_2}}</h5>
                        <div class="tags">
                            {{#page_13_s2}}
                                <a href="{{tagurl}}">{{{tags}}}</a>
                            {{/page_13_s2}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{/page_13_s1_enabled}}
<!-- Blog list End -->
<!-- Cta Start -->
{{#page_13_s2_enabled}}
<section class="cta_box style_2">
    <div class="container relative z-2">
        <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
            <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
            <div class="row justify-content-between align-items-center">
                <div class="col-lg-6">
                    <div class="text_block wow fadeInLeft">
                        <div class="section-title left-align white">
                            <p class="subtitle">
                                <i class="fal fa-book"></i>
                                {{page_13_s2_headline}}
                            </p>
                            <h3 class="title">{{page_13_s2_header}}</h3>
                            <a href="{{{page_13_s2_button_lnk}}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                {{page_13_s2_button}} 
                                <i class="fal fa-chevron-right ml-2"></i>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="image_box relative z-1 wow fadeInRight">
                        <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                        <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                        <div class="arrows slideRight">
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="arrows to_up slideTop">
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
        </div>
    </div>
</section>
{{/page_13_s2_enabled}}
<!-- Cta End -->
