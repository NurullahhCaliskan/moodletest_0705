{{!
    Page Portfolio Gallery.
}}
{{> theme_wellko/frontpage/navbar/header3 }}
<!-- Subheader Start -->
{{< theme_wellko/frontpage/page/page_subhdr }}
    
    {{$page_subhdimg_0}}
        {{#page_18_page_subhdimg}}style="background-image: url({{frontpageimglink}}{{page_18_page_subhdimg}});"{{/page_18_page_subhdimg}}
        {{^page_18_page_subhdimg}}style="padding: 70px 0 28px;"{{/page_18_page_subhdimg}}
    {{/page_subhdimg_0}}    

    {{$page_subhdimg_1}}
        {{^page_18_page_subhdimg}}style="font-size: 45px;"{{/page_18_page_subhdimg}}
    {{/page_subhdimg_1}}

    {{$page_subhdimg_2}}
        {{page_18_page_title}}
    {{/page_subhdimg_2}}

    {{$page_subhdimg_3}}
        {{#page_18_page_subhdimg}}
            {{> theme_wellko/frontpage/page/page_subhdr_elm }}
        {{/page_18_page_subhdimg}}
    {{/page_subhdimg_3}}

{{/ theme_wellko/frontpage/page/page_subhdr }}
<!-- Subheader End -->
{{#page_18_s1_enabled}}
<!-- Section Start -->
<div class="section-padding isotope-filter-items">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="filter-btns mb-5">
                    <ul>
                        <li class="active filter-trigger" data-filter="*">
                            <a href="#">All</a>
                        </li>
                        {{#page_17_s1_g}}
                        <li class="filter-trigger" data-filter=".{{group}}">
                            <a href="#">{{group}}</a>
                        </li>
                        {{/page_17_s1_g}}
                    </ul>
                </div>
            </div>
        </div>
        <div class="row filteritems">
            <!-- Portfolio Start -->
            {{#page_17_s1}}
            <div class="col-lg-4 col-md-6 masonry-item {{group1}} {{group2}} {{group3}}">
                <div class="portfolio_box type_gallery">
                    <div class="portfolio_img">
                        <img src="{{frontpageimglink}}portfolio/{{counter}}.jpg" class="image-fit" alt="img">
                    </div>
                    <a href="{{link}}" class="link">
                        <i class="fal fa-plus"></i>
                    </a>
                </div>
            </div>
            {{/page_17_s1}}
            <!-- Portfolio End -->
        </div>
    </div>
</div>
<!-- Section End -->
{{/page_18_s1_enabled}}

<!-- Cta Start -->
{{#page_18_s2_enabled}}
<section class="cta_box style_2">
    <div class="container relative z-2">
        <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
            <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
            <div class="row justify-content-between align-items-center">
                <div class="col-lg-6">
                    <div class="text_block wow fadeInLeft">
                        <div class="section-title left-align white">
                            <p class="subtitle">
                            <i class="fal fa-book"></i>
                                {{page_18_s2_headline}}
                            </p>
                            <h3 class="title">{{page_18_s2_header}}</h3>
                            <a href="{{page_18_s2_button_lnk}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                {{page_18_s2_button}} 
                                <i class="fal fa-chevron-right ml-2"></i>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="image_box relative z-1 wow fadeInRight">
                        <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                        <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                        <div class="arrows slideRight">
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="arrows to_up slideTop">
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
        </div>
    </div>
</section>
{{/page_18_s2_enabled}}
<!-- Cta End -->
{{#footer_enabled}}
    {{> theme_wellko/frontpage/other/footer03 }}
{{/footer_enabled}}