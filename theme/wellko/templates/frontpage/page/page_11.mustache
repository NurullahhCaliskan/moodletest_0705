{{> theme_wellko/frontpage/navbar/header3 }}
<!-- Subheader Start -->
{{< theme_wellko/frontpage/page/page_subhdr }}
    
    {{$page_subhdimg_0}}
        {{#page_11_page_subhdimg}}style="background-image: url({{frontpageimglink}}{{page_11_page_subhdimg}});"{{/page_11_page_subhdimg}}
        {{^page_11_page_subhdimg}}style="padding: 70px 0 28px;"{{/page_11_page_subhdimg}}
    {{/page_subhdimg_0}}    

    {{$page_subhdimg_1}}
        {{^page_11_page_subhdimg}}style="font-size: 45px;"{{/page_11_page_subhdimg}}
    {{/page_subhdimg_1}}

    {{$page_subhdimg_2}}
        <span lang="en" class="multilang" style="display:none;">{{page_11_page_title_1}}</span>
        <span lang="tr" class="multilang" style="display:none;">{{page_11_page_title_2}}</span>
    {{/page_subhdimg_2}}

    {{$page_subhdimg_3}}
        {{#page_11_page_subhdimg}}
            {{> theme_wellko/frontpage/page/page_subhdr_elm }}
        {{/page_11_page_subhdimg}}
    {{/page_subhdimg_3}}

{{/ theme_wellko/frontpage/page/page_subhdr }}
<!-- Subheader End -->
<!-- Coach Grid Start -->
{{#page_11_s1_enabled}}
<section class="section-padding">
    <div class="container">
        <div class="row">
            <!-- Coach block -->
            {{#h1_08_block}}
            <div class="col-xl-4 col-md-6">
                <div class="coach_block wow fadeInDown" data-wow-delay=".{{counter}}s">
                    <div class="coach_img">
                        {{#imgurl}}
                        <img src="{{imgurl}}" alt="img" class="image-fit">
                        <div class="coach_badges">
                            <span class="tag_badge bg-thm-color-four">{{cost}}{{currency}}</span>
                        </div>
                        {{/imgurl}}
                        <a href="{{coursed_detail_lnk}}" class="thm-btn thm-color-two bg-thm-color-white thm-color-two-shadow btn-circle link">
                            <i class="fal fa-chevron-right"></i>
                        </a>
                    </div>
                    <div class="coach_caption">
                        <div class="coach_meta">
                            <a href="{{categorylink}}" class="coach_cat thm-btn bg-thm-color-three thm-color-three-shadow btn-rounded">
                                {{categoryName}}
                            </a>
                            <div class="ratings">
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                            </div>
                        </div>
                        <h5 class="title mb-xl-20">
                            <a href="{{coursed_detail_lnk}}">
                                {{fullname}}
                            </a>
                        </h5>
                        <div class="author">
                            {{#userpicture}}
                            <img src="{{userpicture}}" alt="img" class="image-fit">
                            <a href="{{team_detail_lnk}}" class="thm-color-one">{{teachername}}</a>
                            {{/userpicture}}
                        </div>
                    </div>
                </div>
            </div>
            {{/h1_08_block}}
            <!-- Coach block -->
        </div>
    </div>
</section>
{{/page_11_s1_enabled}}
<!-- Coach Grid End -->
<!-- Plane Start -->
{{#page_11_s2_enabled}}
<div class="bg-thm-color-two plane_box relative z-1">
    <div class="container relative z-1">
        <div class="row justify-content-center">
            <div class="col-xl-9 col-lg-11 relative z-1">
                <img src="{{frontpageimglink}}elements/element_6.png" class="element_1 rotate_elem" alt="Element">
                <h2 class="thm-color-white">{{h1_12_block_caption}}</h2>
            </div>
        </div>
        <img src="{{frontpageimglink}}elements/element_7.png" class="element_2 zoom-fade" alt="Element">
    </div>
</div>
{{/page_11_s2_enabled}}
<!-- Plane End -->
<!-- Portfolio Start -->
{{#page_11_s3_enabled}}
{{> theme_wellko/frontpage/home_3/block_09 }}
{{/page_11_s3_enabled}}
<!-- Portfolio End -->
<!-- Cta Start -->
{{#page_11_s4_enabled}}
<section class="cta_box style_2">
        <div class="container relative z-2">
            <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
                <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
                <div class="row justify-content-between align-items-center">
                    <div class="col-lg-6">
                        <div class="text_block wow fadeInLeft">
                            <div class="section-title left-align white">
                                <p class="subtitle">
                                <i class="fal fa-book"></i>
                                    {{page_11_s4_headline}}
                                </p>
                                <h3 class="title">{{page_11_s4_header}}</h3>
                                <a href="{{{page_11_s4_button_lnk}}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                    {{page_11_s4_button}} 
                                    <i class="fal fa-chevron-right ml-2"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="image_box relative z-1 wow fadeInRight">
                            <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                            <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                            <div class="arrows slideRight">
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="arrows to_up slideTop">
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
            </div>
        </div>
</section>
{{/page_11_s4_enabled}}
<!-- Cta End -->
{{#footer_enabled}}
    {{> theme_wellko/frontpage/other/footer03 }}
{{/footer_enabled}}