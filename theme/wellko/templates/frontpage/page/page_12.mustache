{{!
    Page Blog Grid.
}}
{{> theme_wellko/frontpage/navbar/header3 }}
<!-- Subheader Start -->
{{< theme_wellko/frontpage/page/page_subhdr }}
    
    {{$page_subhdimg_0}}
        {{#page_12_page_subhdimg}}style="background-image: url({{frontpageimglink}}{{page_12_page_subhdimg}});"{{/page_12_page_subhdimg}}
        {{^page_12_page_subhdimg}}style="padding: 70px 0 28px;"{{/page_12_page_subhdimg}}
    {{/page_subhdimg_0}}    

    {{$page_subhdimg_1}}
        {{^page_12_page_subhdimg}}style="font-size: 45px;"{{/page_12_page_subhdimg}}
    {{/page_subhdimg_1}}

    {{$page_subhdimg_2}}
        {{page_12_page_title}}
    {{/page_subhdimg_2}}

    {{$page_subhdimg_3}}
        {{#page_12_page_subhdimg}}
            {{> theme_wellko/frontpage/page/page_subhdr_elm }}
        {{/page_12_page_subhdimg}}
    {{/page_subhdimg_3}}

{{/ theme_wellko/frontpage/page/page_subhdr }}
<!-- Subheader End -->
<!-- Blog Grid Start -->
{{#page_12_s1_enabled}}
<section class="section-padding">
    <div class="container">
        <div class="row">
            <!-- post -->
            {{#h2_09_block}}
            <div class="col-xl-4 col-md-6">
                <article class="post style_2 wow fadeInDown" data-wow-delay=".{{count}}ms">
                    <div class="post_img">
                     {{#imgurl}}
                        <img src="{{imgurl}}" alt="img" class="image-fit">
                    {{/imgurl}}
                    </div>
                    <div class="post_caption">
                        <ul class="post_meta">
                            <li>
                                <i class="fal fa-calendar-alt"></i>
                                {{lastmodified}}
                            </li>
                            <li>
                                <i class="fal fa-comment-lines"></i>
                                Comments ({{commentscount}})
                            </li>
                        </ul>
                        <h4 class="post_title">
                            <a href="{{post_detail_lnk}}">{{subject}}</a>
                        </h4>
                        <p class="post_text">
                            {{#shortentext}} 125, {{{summary}}} {{/shortentext}}
                        </p>
                        <a href="{{post_detail_lnk}}" class="thm-btn bg-thm-color-white thm-color-one btn-rectangle">Read More 
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </article>
            </div>
            {{/h2_09_block}}
            <!-- post -->
        </div>
    </div>
</section>
{{/page_12_s1_enabled}}
<!-- Blog Grid End -->
<!-- Cta Start -->
{{#page_12_s2_enabled}}
<section class="cta_box style_2">
        <div class="container relative z-2">
            <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
                <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
                <div class="row justify-content-between align-items-center">
                    <div class="col-lg-6">
                        <div class="text_block wow fadeInLeft">
                            <div class="section-title left-align white">
                                <p class="subtitle">
                                <i class="fal fa-book"></i>
                                    {{page_12_s2_headline}}
                                </p>
                                <h3 class="title">{{page_12_s2_header}}</h3>
                                <a href="{{{page_12_s2_button_lnk}}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">{{page_12_s2_button}} 
                                <i class="fal fa-chevron-right ml-2"></i></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="image_box relative z-1 wow fadeInRight">
                            <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                            <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                            <div class="arrows slideRight">
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="arrows to_up slideTop">
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
            </div>
        </div>
</section>
{{/page_12_s2_enabled}}
<!-- Cta End -->
{{#footer_enabled}}
    {{> theme_wellko/frontpage/other/footer03 }}
{{/footer_enabled}}