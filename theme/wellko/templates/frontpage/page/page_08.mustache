<!-- Quote Form Start -->
<section id="page_08_s3" class="section-padding pb-0 section-bg quote_form_sec elements relative z-1" style="background-image: url({{frontpageimglink}}bg/bg_7.jpg);">
    <div class="container relative z-1">
        <div class="row flex-lg-row-reverse align-items-end justify-content-between">
            <div class="col-xl-5 col-lg-6 mb-md-80 wow fadeInRight" data-wow-delay=".30ms">
                <img src="{{frontpageimglink}}quote_img.png" alt="img" class="image-fit-contain">
            </div>
            <div class="col-lg-6">
                <div class="quote_box">
                    <div class="section-title left-align white wow fadeInDown">
                        <p class="subtitle mb-4">
                            <i class="fal fa-book"></i>
                            {{page_08_s3_headline}}
                        </p>
                        <h3 class="title">{{page_08_s3_header}}</h3>
                    </div>
                    <form class="wow fadeInUp" action="{{page_08_s3_button_link}}" method="post">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <input type="text" name="name" class="form-control form-control-custom style_2" placeholder="Your Name" autocomplete="off"/>
                                    <i class="fal fa-user"></i>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <input type="email" name="email" class="form-control form-control-custom style_2" placeholder="Email Address" autocomplete="off"/>
                                    <i class="fal fa-envelope"></i>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <input type="text" name="phone" class="form-control form-control-custom style_2" placeholder="Phone Number" autocomplete="off"/>
                                    <i class="fal fa-phone"></i>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <select class="form-control form-control-custom style_2" name="subject">
                                        <option selected>Subject</option>
                                        <option value="option 1">Option 1</option>
                                        <option value="option 2">Option 2</option>
                                        <option value="option 3">Option 3</option>
                                    </select>
                                    <i class="fal fa-chevron-down"></i>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="form-group">
                                <textarea class="form-control form-control-custom style_2" name="message" placeholder="Write Message" rows="5"></textarea>
                            </div>
                            </div>
                            <div class="col-lg-12">
                                <button type="submit" name="submit" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">Send Message 
                                    <i class="fal fa-chevron-right ml-2"></i>
                                </button>
                            </div>
                        </div>
                        <input type="hidden" name="sesskey" value="{{sesskey}}">
                    </form>
                </div>
            </div>
        </div>
        <img src="{{frontpageimglink}}elements/element_39.png" alt="element" class="element_1 rotate_elem">
        <img src="{{frontpageimglink}}elements/element_40.png" alt="element" class="element_2 zoom-fade">
        <img src="{{frontpageimglink}}elements/element_41.png" alt="element" class="element_3 rotate_elem">
        <img src="{{frontpageimglink}}elements/element_42.png" alt="element" class="element_4 rotate_elem">
    </div>
</section>
<!-- Quote Form End -->
{{/page_08_s3_enabled}}
