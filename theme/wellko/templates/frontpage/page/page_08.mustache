<div class="section-padding faqs_elements page-08">
    <div class="container relative z-1">
        <div class="row justify-content-center">
            <div class="col-xl-9 col-lg-11">
                <ul class="nav nav-tabs style_3 wow fadeInUp">
                    <li class="nav-item">
                        <a href="#" class="nav-link active" data-toggle="tab" onclick="filterSelection('all')">All</a>
                    </li>
                    {{#page_08_s1_1}}
