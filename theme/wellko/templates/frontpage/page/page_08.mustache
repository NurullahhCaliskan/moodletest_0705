                        <a href="#" class="nav-link" data-toggle="tab" onclick="filterSelection('category-id-{{ctgry}}')">{{ctgry}}</a>
                    </li>
                    {{/page_08_s1_1}}
                </ul>
                <div class="tab-content wow fadeInDown">
                    <div class="tab-pane fade active show">
                        {{#page_08_s1}}
                        <div class="faqsfilterdiv accordion accordion-style style_2 mb-xl-30 category-id-{{category}}" id="{{category}}accordion">
                            <div class="card">
                                <div class="card-header">
                                    <button class="btn btn-link collapsed" type="button" data-toggle="collapse" 
                                        data-target="#{{category}}Item{{counter}}" aria-expanded="true" 
                                        aria-controls="{{category}}Item{{counter}}">
                                        {{title}}
                                    </button>
                                </div>
                                <div id="{{category}}Item{{counter}}" class="collapse" aria-labelledby="{{category}}Item{{counter}}" 
                                    data-parent="#{{category}}accordion">
                                    <div class="card-body">
                                        {{caption}}
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/page_08_s1}}
                    </div>
                </div>
            </div>
        </div>
        <img src="{{frontpageimglink}}elements/element_10.png" alt="element" class="element_1 zoom-fade">
        <img src="{{frontpageimglink}}elements/element_35.png" alt="element" class="element_2 zoom-fade">
        <img src="{{frontpageimglink}}elements/element_36.png" alt="element" class="element_3 rotate_elem">
