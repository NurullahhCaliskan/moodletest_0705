{{!
    Page Team Masonry.
}}
{{> theme_wellko/frontpage/navbar/header3 }}
<!-- Subheader Start -->
{{< theme_wellko/frontpage/page/page_subhdr }}
    
    {{$page_subhdimg_0}}
        {{#page_19_page_subhdimg}}style="background-image: url({{frontpageimglink}}{{page_19_page_subhdimg}});"{{/page_19_page_subhdimg}}
        {{^page_19_page_subhdimg}}style="padding: 70px 0 28px;"{{/page_19_page_subhdimg}}
    {{/page_subhdimg_0}}    

    {{$page_subhdimg_1}}
        {{^page_19_page_subhdimg}}style="font-size: 45px;"{{/page_19_page_subhdimg}}
    {{/page_subhdimg_1}}

    {{$page_subhdimg_2}}
        {{page_19_page_title}}
    {{/page_subhdimg_2}}

    {{$page_subhdimg_3}}
        {{#page_19_page_subhdimg}}
            {{> theme_wellko/frontpage/page/page_subhdr_elm }}
        {{/page_19_page_subhdimg}}
    {{/page_subhdimg_3}}

{{/ theme_wellko/frontpage/page/page_subhdr }}
<!-- Subheader End -->
{{#page_19_s1_enabled}}
<!-- Section Start -->
<section class="section-padding">
    <div class="container">
        <div class="row">
            <!-- Portfolio Start -->
            {{#page_17_s1}}
            <div class="{{column}}">
                <div class="portfolio_box type_gallery type_masonry">
                    <div class="portfolio_img">
                        <img src="{{frontpageimglink}}portfolio/masonry/{{counter}}.jpg" class="image-fit" alt="img">
                    </div>
                    <div class="link">
                        <i class="fal fa-plus"></i>
                        <h4 class="title mb-0">
                            <a href="{{link}}">{{title}}</a>
                        </h4>
                        <p>{{category}}</p>
                    </div>
                </div>
            </div>
            {{/page_17_s1}}
            <!-- Portfolio Start -->
            <div class="col-12 text-center mt-5">
                <a href="{{page_19_s1_button_lnk}}" class="thm-btn bg-thm-color-white thm-color-one btn-border btn-rectangle">
                    {{page_19_s1_button}}
                    <i class="fal fa-chevron-right ml-2"></i>
                </a>
            </div>
        </div>
    </div>
</section>
<!-- Section End -->
{{/page_19_s1_enabled}}

<!-- Cta Start -->
{{#page_19_s2_enabled}}
<section class="cta_box style_2">
    <div class="container relative z-2">
        <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
            <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
            <div class="row justify-content-between align-items-center">
                <div class="col-lg-6">
                    <div class="text_block wow fadeInLeft">
                        <div class="section-title left-align white">
                            <p class="subtitle">
                            <i class="fal fa-book"></i>
                                {{page_19_s2_headline}}
                            </p>
                            <h3 class="title">{{page_19_s2_header}}</h3>
                            <a href="{{page_19_s2_button_lnk}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                {{page_19_s2_button}} 
                                <i class="fal fa-chevron-right ml-2"></i>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="image_box relative z-1 wow fadeInRight">
                        <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                        <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                        <div class="arrows slideRight">
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="arrows to_up slideTop">
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
        </div>
    </div>
</section>
{{/page_19_s2_enabled}}
<!-- Cta End -->
{{#footer_enabled}}
    {{> theme_wellko/frontpage/other/footer03 }}
{{/footer_enabled}}