                        <img src="{{frontpageimglink}}icons/icon_14.png" alt="icon" class="image-fit-contain">
                    </div>
                    <div class="text">
                        <h4>{{page_10_s1_maintitle_1}}</h4>
                        <p>{{page_10_s1_caption_1}}</p>
                        <a href="{{page_10_s1_button_lnk_1}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">{{page_10_s1_button_1}} 
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="contact_faq_box shadow_1 wow fadeInUp" data-wow-delay=".40ms">
                    <div class="icon_faq">
                        <img src="{{frontpageimglink}}icons/icon_15.png" alt="icon" class="image-fit-contain">
                    </div>
                    <div class="text">
                        <h4>{{page_10_s1_maintitle_2}}</h4>
                        <p>{{page_10_s1_caption_2}}</p>
                        <a href="{{page_10_s1_button_lnk_2}}" class="thm-btn bg-thm-color-two thm-color-two-shadow btn-rectangle">{{page_10_s1_button_2}}
                            <i class="fal fa-chevron-right ml-2"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
{{/page_10_s1_enabled}}
