<!-- Contact Form Start -->
{{#page_10_s3_enabled}}
<section id="page_10_s3" class="section">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="section-title wow fadeInDown">
                    <p class="subtitle">
                        <i class="fal fa-book"></i>
                        <span lang="en" class="multilang" style="display:none;">{{page_10_s3_headline_1}}</span>
                        <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_headline_2}}</span> 
                        
                    </p>
                    <h3 class="title">
                    <span lang="en" class="multilang" style="display:none;">{{page_10_s3_header_1}}</span>
                    <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_header_2}}</span> 
                    </h3>
                </div>
            </div>
        </div>
        <form class="wow fadeInUp" action="{{page_10_s3_button_lnk}}" method="post" enctype="multipart/form-data">
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group form_style">
                        <label>
                        <span lang="en" class="multilang" style="display:none;">{{page_10_s3_five_text_1}}</span>
                        <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_five_text_6}}</span> 
                        </label>
                        <input type="text" name="name" required="" class="form-control" autocomplete="off" placeholder="{{page_10_s3_five_text_1}}">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group form_style">
                        <label>
                        <span lang="en" class="multilang" style="display:none;">{{page_10_s3_five_text_2}}</span>
                        <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_five_text_7}}</span>
                        </label>
                        <input type="text" name="phone" class="form-control" autocomplete="off" placeholder="{{page_10_s3_five_text_2}}">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group form_style">
                        <label>
                        <span lang="en" class="multilang" style="display:none;">{{page_10_s3_five_text_3}}</span>
                        <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_five_text_8}}</span>
                        </label>
                        <input type="email" name="email" required="" class="form-control" autocomplete="off" placeholder="{{page_10_s3_five_text_3}}">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group form_style">
                        <label>
                        <span lang="en" class="multilang" style="display:none;">{{page_10_s3_five_text_4}}</span>
                        <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_five_text_9}}</span>
                        </label>
                        <input type="text" name="subject" class="form-control" autocomplete="off" placeholder="{{page_10_s3_five_text_4}}">
                    </div>
                </div>
                <div class="col-lg-12">
                    <div class="form-group form_style">
                        <label>
                        <span lang="en" class="multilang" style="display:none;">{{page_10_s3_five_text_5}}</span>
                        <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_five_text_10}}</span>
                        </label>
                        <textarea rows="10" class="form-control" placeholder="Write Message" autocomplete="off" name="{{page_10_s3_five_text_5}}"></textarea>
                    </div>
                </div>
                <div class="col-lg-12 text-center">
                    <button type="submit" name="submit" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                    <span lang="en" class="multilang" style="display:none;">{{page_10_s3_button_1}}</span>
                    <span lang="tr" class="multilang" style="display:none;">{{page_10_s3_button_2}}</span>
                    
                        <i class="fal fa-chevron-right ml-2">
                    </i></button>
                </div>
            </div>
            <input type="hidden" name="f-name" value="CONTACT FORM">
            <input type="hidden" name="sesskey" value="{{sesskey}}">
        </form>
    </div>
</section>
{{/page_10_s3_enabled}}
<!-- Contact Form End -->
