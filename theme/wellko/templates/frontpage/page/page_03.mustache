{{!
    Page Events.
}}
{{> theme_wellko/frontpage/navbar/header3 }}
<!-- Subheader Start -->
{{< theme_wellko/frontpage/page/page_subhdr }}
    
    {{$page_subhdimg_0}}
        {{#page_03_page_subhdimg}}style="background-image: url({{frontpageimglink}}{{page_03_page_subhdimg}});"{{/page_03_page_subhdimg}}
        {{^page_03_page_subhdimg}}style="padding: 70px 0 28px;"{{/page_03_page_subhdimg}}
    {{/page_subhdimg_0}}    

    {{$page_subhdimg_1}}
        {{^page_03_page_subhdimg}}style="font-size: 45px;"{{/page_03_page_subhdimg}}
    {{/page_subhdimg_1}}

    {{$page_subhdimg_2}}
        {{page_03_page_title}}
    {{/page_subhdimg_2}}

    {{$page_subhdimg_3}}
        {{#page_03_page_subhdimg}}
            {{> theme_wellko/frontpage/page/page_subhdr_elm }}
        {{/page_03_page_subhdimg}}
    {{/page_subhdimg_3}}

{{/ theme_wellko/frontpage/page/page_subhdr }}
<!-- Subheader End -->

<section>
    <img src="{{frontpageimglink}}events-banner.png" style="width:100%;height:auto;">
</section>

<!-- Events Start -->
{{#page_03_s1_enabled}}
<section class="section-padding bg-thm-color-two-light">
    <div class="container">
        <div class="page_bar">
            <div class="row align-items-center">
                <div class="col-lg-6 text-center text-lg-left wow fadeInLeft">
                    <p class="thm-font">We found {{page_03_s1_count}} events available for you</p>
                </div>
             </div>
        </div>
        <div class="row">
            <!-- Block -->
            {{#page_03_s1}}
            <div class="col-lg-4 col-md-6">
                <div class="event_block wow fadeInDown" data-wow-delay=".{{counter}}ms">
                    <div class="event_img relative">
                        <img src="{{frontpageimglink}}event/{{counter}}.jpg" alt="img" class="image-fit">
                        <span class="tag_badge style_3 bg-thm-color-four">{{cost}}</span>
                    </div>
                    <div class="event_caption">
                        <div class="event_text">
                            <span class="date thm-color-two font-weight-bold">{{date}}</span>
                            <h5 class="event_title">
                                <a href="{{link}}">{{title}}</a>
                            </h5>
                            <p>
                                <span class="thm-color-two mr-2">
                                    <i class="far fa-map-marker-alt"></i>
                                </span>
                                {{location}}
                            </p>
                        </div>
                        <div class="event_btn">
                            <a href="{{link}}" class="thm-btn bg-thm-color-two-light thm-color-two btn-rectangle btn-small">
                                <i class="fal fa-chevron-right font-weight-bold"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            {{/page_03_s1}}
            <!-- Block -->
        </div>
    </div>
</section>
{{/page_03_s1_enabled}}
<!-- Events End -->
<!-- Sponsors Start -->
{{#page_03_s2_enabled}}
<section class="section-padding">
    <div class="container">
        <div class="row justify-content-around">
            <div class="col-auto">
                <div class="sponsors_img">
                    <img src="{{frontpageimglink}}sponsors/1.png" alt="img">
                </div>
            </div>
            <div class="col-auto">
                <div class="sponsors_img">
                    <img src="{{frontpageimglink}}sponsors/2.png" alt="img">
                </div>
            </div>
            <div class="col-auto">
                <div class="sponsors_img">
                    <img src="{{frontpageimglink}}sponsors/3.png" alt="img">
                </div>
            </div>
            <div class="col-auto">
                <div class="sponsors_img">
                    <img src="{{frontpageimglink}}sponsors/4.png" alt="img">
                </div>
            </div>
            <div class="col-auto">
                <div class="sponsors_img">
                    <img src="{{frontpageimglink}}sponsors/5.png" alt="img">
                </div>
            </div>
        </div>
    </div>
</section>
{{/page_03_s2_enabled}}
<!-- Sponsors End -->
{{#page_03_s3_enabled}}
<!-- Cta Start -->
<div class="cta_box style_2">
    <div class="container relative z-2">
        <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
            <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
            <div class="row justify-content-between align-items-center">
                <div class="col-lg-6">
                    <div class="text_block wow fadeInLeft">
                        <div class="section-title left-align white">
                            <p class="subtitle">
                                <i class="fal fa-book"></i>
                                {{page_03_s3_headline}}
                            </p>
                            <h3 class="title">{{page_03_s3_header}}</h3>
                            <a href="{{page_03_s3_button_link}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                {{page_03_s3_button}} 
                                <i class="fal fa-chevron-right ml-2"></i>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="image_box relative z-1 wow fadeInRight">
                        <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                        <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                        <div class="arrows slideRight">
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="arrows to_up slideTop">
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
        </div>
    </div>
</div>
<!-- Cta End -->
{{/page_03_s3_enabled}}
{{#footer_enabled}}
    {{> theme_wellko/frontpage/other/footer03 }}
{{/footer_enabled}}