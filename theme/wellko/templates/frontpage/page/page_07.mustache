<!-- Pricing -->
{{#page_07_s1_enabled}}
<section class="pricing_sec section">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="section-title wow fadeInDown">
                    <p class="subtitle">
                        <i class="fal fa-book"></i>
                        {{h3_06_block_headline}}
                    </p>
                    <h3 class="title">{{h3_06_block_header}}</h3>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <ul class="nav nav-tabs style_1 border-0 justify-content-center justify-content-lg-end wow fadeInRight">
                    <li class="nav-item">
                        <a href="#monthly" class="nav-link mb-0 border-0 thm-btn bg-thm-color-one  btn-rectangle active" data-toggle="tab">Monthly <i class="fal fa-chevron-right ml-2"></i></a>
                    </li>
                    <li class="nav-item">
                        <a href="#yearly" class="nav-link mb-0 border-0 thm-btn bg-thm-color-one btn-rectangle" data-toggle="tab">Yearly <i class="fal fa-chevron-right ml-2"></i></a>
                    </li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane fade active show" id="monthly">
                        <div class="row no-gutters justify-content-center">
                            {{#h3_06_block}}
                            <div class="col-lg-4 col-md-6">
                                <div class="plan_box {{active_m}} wow fadeInDown">
                                    <h4 class="plan_title">{{title_m}}</h4>
                                    <p class="plan_subtext">{{caption_m}}</p>
                                    <h2 class="plan_price">{{price_m}}</h2>
                                    <ul class="about_list style_2 mb-xl-30">
                                        <li>
                                            {{text_four_m_1}}
                                        </li>
                                        <li>
                                            {{text_four_m_2}}
                                        </li>
                                        <li>
                                            {{text_four_m_3}}
                                        </li>
                                        <li>
                                            {{text_four_m_4}}
                                        </li>
                                    </ul>
                                    <a href="?page=10" class="thm-btn bg-thm-color-two-light thm-color-one btn-rectangle">Select Plan <i class="fal fa-chevron-right ml-2"></i></a>
                                </div>
                            </div>
                            {{/h3_06_block}}
                        </div>
                    </div>
                    <div class="tab-pane fade" id="yearly">
                        <div class="row no-gutters justify-content-center">
                            {{#h3_06_block}}
                            <div class="col-lg-4 col-md-6">
                                <div class="plan_box {{active_y}} wow fadeInUp">
                                    <h4 class="plan_title">{{title_y}}</h4>
                                    <p class="plan_subtext">{{caption_y}}</p>
                                    <h2 class="plan_price">{{price_y}}</h2>
                                    <ul class="about_list style_2 mb-xl-30">
                                        <li>
                                            {{text_four_y_1}}
                                        </li>
                                        <li>
                                            {{text_four_y_2}}
                                        </li>
                                        <li>
                                            {{text_four_y_3}}
                                        </li>
                                        <li>
                                            {{text_four_y_4}}
                                        </li>
                                    </ul>
                                    <a href="?page=10" class="thm-btn bg-thm-color-two-light thm-color-one btn-rectangle">Select Plan <i class="fal fa-chevron-right ml-2"></i></a>
                                </div>
                            </div>
                            {{/h3_06_block}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
{{/page_07_s1_enabled}}
<!-- Pricing End -->
<!-- Portfolio Start -->
{{#page_07_s2_enabled}}
<section class="section-padding pt-0">
    <div class="container">
        <div class="row align-items-end">
            <div class="col-lg-7">
                <div class="section-title left-align wow fadeInLeft">
                    <p class="subtitle">
                        <i class="fal fa-book"></i>
                        {{h3_09_block_headline}}
                    </p>
                    <h3 class="title">{{h3_09_block_header}}</h3>
                </div>
            </div>
            <div class="col-lg-5 text-left text-lg-right wow fadeInRight">
                <a href="{{h3_09_block_btn_link}}" class="thm-btn bg-thm-color-white thm-color-one btn-border btn-rectangle mb-xl-60">
                    {{h3_09_block_btn}}
                    <i class="fal fa-chevron-right ml-2"></i>
                </a>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6">
                <div class="portfolio_box wow fadeInUp" data-wow-delay=".20ms">
                    <div class="portfolio_img">
                        <img src="{{frontpageimglink}}portfolio/1.jpg" class="image-fit" alt="img">
                    </div>
                    <div class="portfolio_caption">
                        <div class="text_box">
                            <h4 class="title mb-0">
                                <a href="{{h3_09_block_caption_one_lnk}}">{{h3_09_block_caption_one}}</a>
                            </h4>
                            <p>{{h3_09_block_job_one}}</p>
                        </div>
                        <div class="btn_box mb-xl-30">
                            <a href="{{h3_09_block_caption_one_lnk}}" class="thm-btn bg-thm-color-two-light thm-color-two btn-rectangle btn-small">
                                <i class="fal fa-chevron-right font-weight-bold"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="portfolio_box wow fadeInDown" data-wow-delay=".40ms">
                    <div class="portfolio_img">
                        <img src="{{frontpageimglink}}portfolio/2.jpg" class="image-fit" alt="img">
                    </div>
                    <div class="portfolio_caption">
                        <div class="text_box">
                            <h4 class="title mb-0">
                                <a href="{{h3_09_block_caption_two_lnk}}">{{h3_09_block_caption_two}}</a>
                            </h4>
                            <p>{{h3_09_block_job_two}}</p>
                        </div>
                        <div class="btn_box mb-xl-30">
                            <a href="{{h3_09_block_caption_two_lnk}}" class="thm-btn bg-thm-color-two-light thm-color-two btn-rectangle btn-small">
                                <i class="fal fa-chevron-right font-weight-bold"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
{{/page_07_s2_enabled}}
<!-- Portfolio End -->
<!-- Cta Start -->
{{#page_07_s3_enabled}}
<div class="cta_box style_2">
    <div class="container relative z-2">
        <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
            <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
            <div class="row justify-content-between align-items-center">
                <div class="col-lg-6">
                    <div class="text_block wow fadeInLeft">
                        <div class="section-title left-align white">
                            <p class="subtitle">
                            <i class="fal fa-book"></i>
                                {{page_07_s3_headline}}
                            </p>
                            <h3 class="title">{{page_07_s3_header}}</h3>
                            <a href="{{{page_07_s3_button_link}}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                {{page_07_s3_button}} 
                                <i class="fal fa-chevron-right ml-2"></i>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="image_box relative z-1 wow fadeInRight">
                        <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                        <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                        <div class="arrows slideRight">
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                            <div class="arrow"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="arrows to_up slideTop">
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
            <div class="arrow"></div>
        </div>
    </div>
</div>
{{/page_07_s3_enabled}}
