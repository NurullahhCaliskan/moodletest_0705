<!-- Video Start -->
{{#page_04_s1_enabled}}
<section class="section-padding isotope-filter-items">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="filter-btns mb-5 wow fadeInDown">
                    <ul>
                        <li class="active filter-trigger" data-filter="*">
                            <a href="#">All</a>
                        </li>
                        {{#page_04_s1_ctg}}
                        <li class="filter-trigger" data-filter=".{{category}}">
                            <a href="#">{{category}}</a>
                        </li>
                        {{/page_04_s1_ctg}}
                    </ul>
                </div>
            </div>
        </div>
        <div class="row filteritems">
            <!-- Box Start -->
            {{#page_04_s1}}
            <div class="col-lg-4 col-md-6 masonry-item {{category}}">
                <div class="video_box wow fadeInUp" data-wow-delay=".{{counter}}ms">
                    <img src="{{frontpageimglink}}video/{{counter}}.jpg" alt="img" class="image-fit">
                    <div class="video_badges">
                    {{#badge}}
                        <span class="tag_badge bg-thm-color-four">{{badge}}</span>
                    {{/badge}}
                    </div>
                    <a href="{{linkurl}}" class="popup-youtube video_btn">
                        <i class="fal fa-play"></i>
                    </a>
                    <div class="video_text">
                        <h6 class="text-white mb-0">{{firsttitle}}</h6>
                        <p class="mb-0 thm-color-light">{{secondtitle}}</p>
                    </div>
                </div>
            </div>
            {{/page_04_s1}}
            <!-- Box End -->
        </div>
        <div class="row filteritems">
            <!-- Box Start -->
            {{#page_04_s1_}}
            <div class="col-xl-3 col-lg-4 col-md-6 masonry-item {{category}}">
                <div class="video_box wow fadeInDown" data-wow-delay=".{{counter}}ms">
                    <img src="{{frontpageimglink}}video/{{counter}}.jpg" alt="img" class="image-fit">
                    <div class="video_badges">
                    {{#badge}}
                        <span class="tag_badge bg-thm-color-four">{{badge}}</span>
                    {{/badge}}
                    </div>
                    <a href="{{linkurl}}" class="popup-youtube video_btn">
                        <i class="fal fa-play"></i>
                    </a>
                    <div class="video_text">
                        <h6 class="text-white mb-0">{{firsttitle}}</h6>
                        <p class="mb-0 thm-color-light">{{secondtitle}}</p>
                    </div>
                </div>
            </div>
            {{/page_04_s1_}}
            <!-- Box End -->
        </div>
        <div class="row filteritems">
            <!-- Box Start -->
            {{#page_04_s1__}}
            <div class="col-md-6 masonry-item {{category}}">
                <div class="video_box wow fadeInDown" data-wow-delay=".{{counter}}ms">
                    <img src="{{frontpageimglink}}video/{{counter}}.jpg" alt="img" class="image-fit">
                    <div class="video_badges">
                    {{#badge}}
                        <span class="tag_badge bg-thm-color-four">{{badge}}</span>
                    {{/badge}}
                    </div>
                    <a href="{{linkurl}}" class="popup-youtube video_btn">
                        <i class="fal fa-play"></i>
                    </a>
                    <div class="video_text">
                        <h6 class="text-white mb-0">{{firsttitle}}</h6>
                        <p class="mb-0 thm-color-light">{{secondtitle}}</p>
                    </div>
                </div>
            </div>
            {{/page_04_s1__}}
            <!-- Box End -->
        </div>
    </div>
</section>
{{/page_04_s1_enabled}}
<!-- Video End -->
<!-- Plane Start -->
{{#page_04_s2_enabled}}
<div class="bg-thm-color-two plane_box relative z-1">
    <div class="container relative z-1">
        <div class="row justify-content-center">
            <div class="col-xl-9 col-lg-11 relative z-1">
                <img src="{{frontpageimglink}}elements/element_6.png" class="element_1 rotate_elem" alt="Element">
                <h2 class="thm-color-white">{{page_04_s2_caption}}</h2>
            </div>
        </div>
        <img src="{{frontpageimglink}}elements/element_7.png" class="element_2 zoom-fade" alt="Element">
    </div>
</div>
{{/page_04_s2_enabled}}
<!-- Plane End -->
<!-- Coach box Start -->
{{#page_04_s3_enabled}}
<section class="section wide_fluid_container">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="section-title wow fadeInDown">
                    <p class="subtitle">
                        <i class="fal fa-book"></i>
                        {{h2_04_block_headline}}
                    </p>
                    <h3 class="title">{{h2_04_block_header}}</h3>
                </div>
            </div>
        </div>
        <div class="coach_slider row wow fadeInUp">
            <!-- Coach block -->
            {{#h2_04_block}}
            <div class="col-12 slide-item">
                <div class="coach_block">
                    <div class="coach_img">
                        {{#imgurl}}
                        <img src="{{imgurl}}" alt="img" class="image-fit">
                        {{/imgurl}}
                        <div class="coach_badges">
                            <span class="tag_badge bg-thm-color-four">{{cost}}{{currency}}</span>
                        </div>
                        <a href="{{coursed_detail_lnk}}" class="thm-btn thm-color-two bg-thm-color-white thm-color-two-shadow btn-circle link">
                            <i class="fal fa-chevron-right"></i>
                        </a>
                    </div>
                    <div class="coach_caption">
                        <div class="coach_meta">
                            <a href="{{categorylink}}" class="coach_cat thm-btn bg-thm-color-three thm-color-three-shadow btn-rounded">{{categoryName}}</a>
                            <div class="ratings">
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                                <i class="fal fa-star active"></i>
                            </div>
                        </div>
                        <h5 class="title mb-xl-20">
                            <a href="{{coursed_detail_lnk}}">
                                {{fullname}}
                            </a>
                        </h5>
                        <div class="author">
                            {{#userpicture}}
                            <img src="{{userpicture}}" alt="img" class="image-fit">
                            <a href="{{team_detail_lnk}}" class="thm-color-one">{{teachername}}</a>
                            {{/userpicture}}    
                        </div>
                    </div>
                </div>
            </div>
            {{/h2_04_block}}
            <!-- Coach block -->
        </div>
    </div>
</section>
{{/page_04_s3_enabled}}
<!-- Coach box End -->
<!-- Cta Start -->
{{#page_04_s4_enabled}}
<section class="cta_box style_2">
        <div class="container relative z-2">
            <div class="bg-thm-color-two cta_box_inner z-2 mb-negative">
                <img src="{{frontpageimglink}}elements/shape.png" alt="shape" class="shape_1 zoom-fade">
                <div class="row justify-content-between align-items-center">
                    <div class="col-lg-6">
                        <div class="text_block wow fadeInLeft">
                            <div class="section-title left-align white">
                                <p class="subtitle">
                                <i class="fal fa-book"></i>
                                    {{page_04_s4_headline}}
                                </p>
                                <h3 class="title">{{page_04_s4_header}}</h3>
                                <a href="{{{page_04_s4_button_link}}}" class="thm-btn bg-thm-color-three thm-color-three-shadow btn-rectangle">
                                    {{page_04_s4_button}} 
                                    <i class="fal fa-chevron-right ml-2"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="image_box relative z-1 wow fadeInRight">
                            <img src="{{frontpageimglink}}elements/square_shape.png" alt="shape" class="shape_2 rotate_elem">
                            <img src="{{frontpageimglink}}cta_image.png" alt="img" class="image-fit">
                            <div class="arrows slideRight">
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                                <div class="arrow"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="arrows to_up slideTop">
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
                <div class="arrow"></div>
            </div>
        </div>
</section>
{{/page_04_s4_enabled}}
<!-- Cta End -->
